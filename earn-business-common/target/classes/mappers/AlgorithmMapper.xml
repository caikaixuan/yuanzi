<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ygbc.brain.business.common.dal.dao.AlgorithmDao">
    <resultMap id="BaseResultMap" type="com.ygbc.brain.business.common.dal.data.AlgorithmData">
        <id column="id" property="id" jdbcType="BIGINT"/>
        <result column="code" property="code" jdbcType="VARCHAR"/>
        <result column="name" property="name" jdbcType="VARCHAR"/>
        <result column="name_cn" property="nameCn" jdbcType="VARCHAR"/>
        <result column="name_en" property="nameEn" jdbcType="VARCHAR"/>
        <result column="remark" property="remark" jdbcType="VARCHAR"/>
        <result column="arguments" property="arguments" jdbcType="VARCHAR"/>
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
        <result column="create_by" property="createBy" jdbcType="VARCHAR"/>
        <result column="update_time" property="updateTime" jdbcType="TIMESTAMP"/>
        <result column="update_by" property="updateBy" jdbcType="VARCHAR"/>
        <result column="ver" property="ver" jdbcType="INTEGER"/>
        <result column="is_del" property="isDel" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id, code, name, name_cn, name_en, remark,arguments,
        create_time,create_by, update_time, update_by, ver, is_del
    </sql>

    <sql id="SQL_FILTER">
        <where>
            <if test="id != null">
                and id = #{id,jdbcType=BIGINT}
            </if>
            <if test="code != null">
                and code = #{code,jdbcType=VARCHAR}
            </if>
            <if test="name != null">
                and name = #{name,jdbcType=VARCHAR}
            </if>
            <if test="nameCn != null">
                and name_cn = #{nameCn,jdbcType=VARCHAR}
            </if>
            <if test="nameEn != null">
                and name_en = #{nameEn,jdbcType=VARCHAR}
            </if>
            <if test="remark != null">
                and remark = #{remark,jdbcType=VARCHAR}
            </if>
            <if test="arguments != null">
                and arguments = #{arguments,jdbcType=VARCHAR}
            </if>
            <if test="createTime != null">
                <![CDATA[   and DATE_FORMAT(create_time, '%Y-%m-%d %H:%I:%S') >=  DATE_FORMAT(#{createTime}, '%Y-%m-%d %H:%I:%S')   ]]>
            </if>
            <if test="createBy != null">
                and create_by = #{createBy,jdbcType=VARCHAR}
            </if>
            <if test="updateTime != null">
                <![CDATA[   and DATE_FORMAT(update_time, '%Y-%m-%d %H:%I:%S') >=  DATE_FORMAT(#{updateTime}, '%Y-%m-%d %H:%I:%S')   ]]>
            </if>
            <if test="updateBy != null">
                and update_by = #{updateBy,jdbcType=VARCHAR}
            </if>
            <if test="ver != null">
                and ver = #{ver,jdbcType=INTEGER}
            </if>
            <if test="isDel == null">
                and is_del = 0
            </if>
            <if test="isDel != null">
                and is_del = #{isDel,jdbcType=TINYINT}
            </if>
            and 1 = 1
        </where>
    </sql>

    <select id="selectById" resultMap="BaseResultMap" parameterType="java.lang.Long">
        select
        <include refid="Base_Column_List"/>
        from algorithm
        where id = #{id,jdbcType=BIGINT} and 1 = 1
    </select>

    <select id="selectOne" resultMap="BaseResultMap" parameterType="java.util.Map">
        select
        <include refid="Base_Column_List"/>
        from algorithm
        <include refid="SQL_FILTER"/>
    </select>

    <select id="selectCount" resultType="java.lang.Integer" parameterType="java.util.Map">
        select
        count(1)
        from algorithm
        <include refid="SQL_FILTER"/>
    </select>

    <select id="select" resultMap="BaseResultMap" parameterType="java.util.Map">
        select
        <include refid="Base_Column_List"/>
        from algorithm
        <include refid="SQL_FILTER"/>
    </select>

    <insert id="insert" parameterType="com.ygbc.brain.business.common.dal.data.AlgorithmData"
            useGeneratedKeys="true" keyProperty="id">
        $plugin.insert(algorithm)
    </insert>

    <insert id="insertForce" parameterType="com.ygbc.brain.business.common.dal.data.AlgorithmData"
            useGeneratedKeys="true" keyProperty="id">
        $plugin.insertForce(algorithm)
    </insert>

    <update id="update" parameterType="com.ygbc.brain.business.common.dal.data.AlgorithmData">
        $plugin.update(algorithm)
    </update>

    <update id="updateForce" parameterType="com.ygbc.brain.business.common.dal.data.AlgorithmData">
        $plugin.updateForce(algorithm)
    </update>

    <delete id="delete" parameterType="com.ygbc.brain.business.common.dal.data.AlgorithmData">
        $plugin.delete(algorithm)
    </delete>
</mapper>


